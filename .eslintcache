= async () => {\n            try {\n                const result = await axios.get(`${API_URL}movie/popular/?api_key=${API_KEY}&language=en-US&page=1`);\n\n                setMovie({\n                    ...movie,\n                    listMovie: result.data.results\n                })\n            } catch (err) {\n                console.log(err);\n            }\n        }\n\n        getList();\n    }, [])\n\n    const LoadMoreButton = async () => {\n        setLoad({\n            loading: true,\n        })\n\n        const result = await axios.get(`${API_URL}movie/popular/?api_key=${API_KEY}&language=en-US&page=${movie.currentPage + 1}`);\n\n        setMovie({\n            listMovie: [...movie.listMovie, ...result.data.results],\n            currentPage: result.data.page,\n            totalPages: result.data.total_pages\n        })\n\n        setLoad({\n            loading: false,\n        })\n    }\n\n    const getSearch = async ({ searchTerm }) => {\n        setLoad({\n            loading: true\n        })\n\n        const result = await axios.get(`${API_URL}search/movie?api_key=${API_KEY}&language=en-US&query=${searchTerm}&page=1&include_adult=false`);\n\n        setMovie({\n            listMovie: [...movie.listMovie || result.data.results],\n            // searchTerm:\n        })\n    }\n\n    return <>\n        <SearchBar callback={getSearch} />\n        <section className=\"popular\">\n            <h2 className=\"popular__title\">\n                Popular Movie\n            </h2>\n\n            <div className=\"popular__grid\">\n                <div className=\"row\">\n                    {\n                        movie.listMovie.map((item, index) => (\n                            <div className=\"col-lg-3 col-md-4 col-sm-6 mb-4\" key={index}>\n                                <div className=\"popular__grid-thumb\">\n                                    <Link to={`${item.id}`} >\n                                        <img src={`${IMAGE_URL}${BACKDROP_SIZE}${item.poster_path}`} className=\"img-fluid\" alt={item.title} />\n                                    </Link>\n                                </div>\n\n                                <div className=\"popular__grid-title\">\n                                    <Link to={`${item.id}`}\n                                        className=\"popular__grid-title-link\">\n                                        {item.title}\n                                    </Link>\n                                </div>\n\n\n                                <div className=\"popular__grid-date\">\n                                    <p className=\"m-0\">\n                                        <span>release date</span>:\n                                        {item.release_date}\n                                    </p>\n                                </div>\n                            </div>\n                        ))\n                    }\n                </div>\n\n                {\n                    load.loading ? <Spinner /> : null\n                }\n                {movie.currentPage <= movie.totalPages &&\n                    !load.loading ? (\n                        <LoadMore onClick={LoadMoreButton} />\n                    ) : null}\n            </div>\n        </section>\n    </>\n}\n\nexport default PopularOverview;","/home/longdang0412/reactjs/movie-app/src/pages/movie.component.jsx",[],"/home/longdang0412/reactjs/movie-app/src/components/movie-detail/movie-detail.component.jsx",[],"/home/longdang0412/reactjs/movie-app/src/components/movie-detail-genre/movie-detail-genre.component.jsx",["79"],"import { useState, useEffect } from 'react'\nimport axios from 'axios'\nimport { useParams } from 'react-router-dom'\nimport { API_KEY, API_URL } from '../../config/config'\n\nconst MovieDetailGenres = () => {\n\n    const [state, setState] = useState({\n        item: []\n    })\n\n    const { id } = useParams();\n\n    useEffect(() => {\n        const getDetail = async () => {\n            try {\n                const response = await axios(`${API_URL}movie/${id}?api_key=${API_KEY}&language=en-US`)\n\n                setState({\n                    item: response.data.genres\n                })\n            } catch (err) {\n                console.log(err);\n            }\n        }\n        getDetail();\n    }, [])\n\n    return <>\n        <ul className=\"d-flex m-0 list-unstyled\">\n            {\n                state.item.map((row, index) => (\n                    <li key={index}>\n                        {row.name}\n                    </li>\n                ))\n            }\n\n        </ul>\n    </>\n}\n\nexport default MovieDetailGenres;","/home/longdang0412/reactjs/movie-app/src/components/movie-detail-actor/movie-detail-actor.component.jsx",["80"],"import { useState, useEffect } from 'react'\nimport axios from 'axios'\nimport { useParams } from 'react-router-dom'\nimport { API_KEY, API_URL, IMAGE_URL, BACKDROP_SIZE } from '../../config/config'\n\nconst MovieDetailActors = () => {\n\n    const [state, setState] = useState({\n        item: []\n    })\n\n    const { id } = useParams();\n\n    useEffect(() => {\n        const getDetail = async () => {\n            try {\n                const response = await axios(`${API_URL}movie/${id}/credits?api_key=${API_KEY}&language=en-US`)\n\n                setState({\n                    item: response.data.cast\n                })\n            } catch (err) {\n                console.log(err);\n            }\n        }\n        getDetail();\n    }, [])\n\n    return <>\n        <div className=\"row mt-4\">\n            {\n                state.item.map((row, index) => (\n                   <div className=\"col-md-3 col-6 mb-4\" key={index}>\n                       <img src={`${IMAGE_URL}${BACKDROP_SIZE}${row.profile_path}`} className=\"img-fluid\" alt={row.name}/>\n\n                       <div className=\"name\">\n                           {row.name}\n                       </div>\n  