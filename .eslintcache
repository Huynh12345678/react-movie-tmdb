nst result = await axios.get(`${API_URL}movie/popular/?api_key=${API_KEY}&language=en-US&page=1`);\n\n                setMovie({\n                    ...movie,\n                    listMovie: result.data.results\n                })\n            } catch (err) {\n                console.log(err);\n            }\n        }\n\n        getList();\n    }, [])\n\n    const LoadMoreButton = async () => {\n        setLoad({\n            loading: true,\n        })\n\n        const result = await axios.get(`${API_URL}movie/popular/?api_key=${API_KEY}&language=en-US&page=${movie.currentPage + 1}`);\n\n        setMovie({\n            listMovie: [...movie.listMovie, ...result.data.results],\n            currentPage: result.data.page,\n            totalPages: result.data.total_pages\n        })\n\n        setLoad({\n            loading: false,\n        })\n    }\n\n    const getSearch = async ({ searchTerm }) => {\n        setLoad({\n            loading: true\n        })\n\n        const result = await axios.get(`${API_URL}search/movie?api_key=${API_KEY}&language=en-US&query=${searchTerm}&page=1&include_adult=false`);\n\n        setMovie({\n            listMovie: [...movie.listMovie || result.data.results],\n            // searchTerm:\n        })\n    }\n\n    return <>\n        <SearchBar callback={getSearch} />\n        <section className=\"popular\">\n            <h2 className=\"popular__title\">\n                Popular Movie\n            </h2>\n\n            <div className=\"popular__grid\">\n                <div className=\"row\">\n                    {\n                        movie.listMovie.map((item, index) => (\n                            <div className=\"col-lg-3 col-md-4 col-sm-6 mb-4\" key={index}>\n                                <div className=\"popular__grid-thumb\">\n                                    <Link to={`${item.id}`} >\n                                        <img src={`${IMAGE_URL}${BACKDROP_SIZE}${item.poster_path}`} className=\"img-fluid\" alt={item.title} />\n                                    </Link>\n                                </div>\n\n                                <div className=\"popular__grid-title\">\n                                    <Link to={`${item.id}`}\n                                        className=\"popular__grid-title-link\">\n                                        {item.title}\n                                    </Link>\n                                </div>\n\n\n                                <div className=\"popular__grid-date\">\n                                    <p className=\"m-0\">\n                                        <span>release date</span>:\n                                        {item.release_date}\n                                    </p>\n                                </div>\n                            </div>\n                        ))\n                    }\n                </div>\n\n                {\n                    load.loading ? <Spinner /> : null\n                }\n                {movie.currentPage <= movie.totalPages &&\n                    !load.loading ? (\n                        <LoadMore onClick={LoadMoreButton} />\n                    ) : null}\n            </div>\n        </section>\n    </>\n}\n\nexport default PopularOverview;","/home/longdang0412/reactjs/movie-app/src/pages/movie.component.jsx",[],["77","78"],"/home/longdang0412/reactjs/movie-app/src/components/movie-detail/movie-detail.component.jsx",[],"/home/longdang0412/reactjs/movie-app/src/components/movie-detail-genre/movie-detail-genre.component.jsx",["79"],"/home/longdang0412/reactjs/movie-app/src/components/movie-detail-actor/movie-detail-actor.component.jsx",["80"],"/home/longdang0412/reactjs/movie-app/src/reportWebVitals.js",[],"/home/longdang0412/reactjs/movie-app/src/components/movie-detail-info/movie-detail-info.component.jsx",["81"],"import { useState, useEffect } from 'react'\nimport axios from 'axios'\nimport './movie-detail-info.styles.scss'\nimport { useParams 